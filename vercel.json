{
  "buildCommand": "npm run build",
  "devCommand": "next dev",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "ignoreCommand": "exit 0",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "build": {
    "env": {
      "NEXT_PUBLIC_VERCEL_ENV": "production",
      "NEXT_SKIP_PRERENDER_ERRORS": "1"
    }
  }
}